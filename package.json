{
  "name": "vue-smart-widget",
  "description": "Smart widget is a flexible and extensible content container component for Vue3.x.",
  "displayName": "VueSmartWidget",
  "version": "1.0.0-alpha.1",
  "author": "xiaoluoboding <xiaoluoboding@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vue-tsc --noEmit && vite build --mode lib",
    "serve": "vite preview",
    "prerelease": "sh prerelease.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaoluoboding/vue-smart-widget.git"
  },
  "homepage": "https://github.com/xiaoluoboding/vue-smart-widget",
  "files": [
    "lib"
  ],
  "main": "./lib/vue-smart-widget.umd.js",
  "module": "./lib/vue-smart-widget.es.js",
  "exports": {
    ".": {
      "import": "./lib/vue-smart-widget.es.js",
      "require": "./lib/vue-smart-widget.umd.js"
    }
  },
  "keywords": [
    "widget",
    "card",
    "panel",
    "vue",
    "grid",
    "drag",
    "draggable",
    "resize",
    "resizable"
  ],
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "license": "MIT",
  "dependencies": {
    "element-resize-detector": "^1.2.3",
    "interactjs": "^1.10.11",
    "mitt": "^3.0.0",
    "vue": "^3.2.6"
  },
  "devDependencies": {
    "@types/element-resize-detector": "^1.1.3",
    "@types/node": "^16.9.1",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compiler-sfc": "^3.2.6",
    "cross-env": "^7.0.3",
    "echarts": "^5.2.0",
    "element-plus": "^1.1.0-beta.12",
    "highlight.js": "^11.2.0",
    "prettier": "^2.4.0",
    "sass": "^1.41.0",
    "typescript": "^4.3.2",
    "ve-charts": "1.0.0-alpha.9",
    "vite": "^2.5.4",
    "vue-router": "4",
    "vue-tsc": "^0.2.2"
  }
}